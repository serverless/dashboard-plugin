{
  "name": "@serverless/enterprise-plugin",
  "version": "1.0.5",
  "engines": {
    "node": ">=6.0"
  },
  "description": "The Serverless Enterprise plugin",
  "main": "dist/index.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "uglify": "./node_modules/uglify-es/bin/uglifyjs dist/index.js --output dist/index.js --beautify",
    "webpack": "webpack",
    "lint": "if [ -z \"$CI\" ]; then eslint src integration-testing/*.js --fix --cache; else eslint src integration-testing/*.js; fi",
    "lint:staged": "lint-staged",
    "test": "jest src --coveragePathIgnorePatterns='.*\\.test\\.js'",
    "integration-test": "jest integration-testing --coveragePathIgnorePatterns='.*\\.test\\.js'"
  },
  "dependencies": {
    "@serverless/event-mocks": "^1.1.1",
    "@serverless/platform-sdk": "^1.0.0",
    "chalk": "^2.4.2",
    "flat": "^4.1.0",
    "fs-extra": "^7.0.1",
    "iso8601-duration": "^1.1.7",
    "jsonata": "^1.6.4",
    "jszip": "^3.2.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-dir": "^0.1.17",
    "node-fetch": "^2.3.0",
    "semver": "^5.6.0",
    "yamljs": "^0.3.0",
    "regenerator-runtime": "^0.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "webpack": "4.24.0"
  },
  "author": "",
  "license": "Apache-2.0",
  "lint-staged": {
    "*.js": [
      "eslint --fix src",
      "eslint --fix sdk-js/src",
      "git add -A"
    ]
  },
  "pre-commit": [
    "lint:staged"
  ]
}
