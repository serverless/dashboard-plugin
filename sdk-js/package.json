{
  "private": true,
  "name": "@serverless/sdk-js",
  "version": "0.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "NODE_ENV=production npm run folder && npm run webpack",
    "cover": "nyc npm test",
    "folder": "rm -rf dist && mkdir dist",
    "test": "mocha \"!(node_modules|dist)/**/*.test.js\"",
    "webpack": "webpack"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@serverless/platform-client": "^1.0.8",
    "after-all-results": "^2.0.0",
    "flat": "^5.0.0",
    "lodash": "^4.17.15",
    "require-in-the-middle": "^5.0.3",
    "stackman": "^4.0.1",
    "type": "^2.0.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@serverless/test": "^4.2.0",
    "chai": "^4.2.0",
    "git-list-updated": "^1.2.1",
    "mocha": "^6.2.3",
    "nyc": "^15.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "mocha": {
    "reporter": "@serverless/test/setup/mocha-reporter",
    "require": [
      "@serverless/test/setup/log",
      "@serverless/test/setup/async-leaks-detector",
      "@serverless/test/setup/mock-homedir",
      "@serverless/test/setup/restore-cwd",
      "@serverless/test/setup/restore-env"
    ],
    "timeout": 5000
  },
  "nyc": {
    "all": true,
    "exclude": [
      "**/*.test.js",
      "coverage/**",
      "dist/**",
      "node_modules/**",
      "prettier.config.js",
      "webpack.config.js"
    ],
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ]
  }
}
