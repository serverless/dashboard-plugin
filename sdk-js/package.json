{
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "cover": "nyc npm test",
    "test": "mocha \"!(node_modules|dist)/**/*.test.js\"",
    "test:isolated": "mocha-isolated \"!(node_modules|dist)/**/*.test.js\""
  },
  "dependencies": {
    "@serverless/platform-client": "^1.1.10",
    "after-all-results": "^2.0.0",
    "flat": "^5.0.2",
    "lodash": "^4.17.20",
    "require-in-the-middle": "^5.0.3",
    "stackman": "^4.0.1",
    "type": "^2.1.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@serverless/test": "^4.9.2",
    "chai": "^4.2.0",
    "git-list-updated": "^1.2.1",
    "mocha": "^6.2.3",
    "nyc": "^15.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "mocha": {
    "reporter": "@serverless/test/setup/mocha-reporter",
    "require": [
      "@serverless/test/setup/log",
      "@serverless/test/setup/async-leaks-detector",
      "@serverless/test/setup/mock-homedir",
      "@serverless/test/setup/restore-cwd",
      "@serverless/test/setup/restore-env"
    ],
    "timeout": 5000
  },
  "nyc": {
    "all": true,
    "exclude": [
      "**/*.test.js",
      "coverage/**",
      "dist/**",
      "node_modules/**",
      "prettier.config.js",
      "webpack.config.js"
    ],
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ]
  }
}
